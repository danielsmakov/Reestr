@model List<Reestr.BLL.DTOs.OrganizationDTO>
@{
    ViewBag.Title = "Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Success</h1>

<h2>@ViewBag.SuccessMessage</h2>

<table>
    <tr>
        <th>Name</th>
        <th>BIN</th>
        <th>Phone Number</th>
        <th>Begin Date</th>
    </tr>
    @foreach (var entity in Model)
    {
        <tr>
            <td>@entity.Name</td>
            <td>@entity.BIN</td>
            <td>@entity.PhoneNumber</td>
            <td>@entity.BeginDate</td>
        </tr>
    }
</table>